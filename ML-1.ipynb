   "source": [
    "# 1) Missing values to mean\n",
    "l = [10,None,30,None,50]\n",
    "valid=[x for x in l if x is not None]\n",
    "mean = sum(valid)/len(valid)\n",
    "for i in range(len(l)):\n",
    "    if l[i] is None:\n",
    "        l[i] = mean\n",
    "print(l)"
   ]
